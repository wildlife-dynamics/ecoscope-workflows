    "ecomaps": {
        "$defs": {
            "LayerStyleProperty": {
                "properties": {
                    "auto_highlight": {
                        "default": false,
                        "title": "Auto Highlight",
                        "type": "boolean"
                    },
                    "opacity": {
                        "default": 1,
                        "title": "Opacity",
                        "type": "number"
                    },
                    "pickable": {
                        "default": true,
                        "title": "Pickable",
                        "type": "boolean"
                    }
                },
                "title": "LayerStyleProperty",
                "type": "object"
            },
            "NorthArrowProperty": {
                "properties": {
                    "placement": {
                        "default": "top-left",
                        "title": "Placement",
                        "type": "string"
                    }
                },
                "title": "NorthArrowProperty",
                "type": "object"
            }
        },
        "additionalProperties": false,
        "properties": {
            "data_type": {
                "enum": [
                    "Scatterplot",
                    "Path",
                    "Polygon"
                ],
                "title": "Data Type",
                "type": "string",
                "description": "The type of visualization."
            },
            "style_props": {
                "allOf": [
                    {
                        "$ref": "#/$defs/LayerStyleProperty"
                    }
                ],
                "default": {
                    "auto_highlight": false,
                    "opacity": 1.0,
                    "pickable": true
                },
                "title": "Style Props",
                "description": "Style arguments for the data visualization."
            },
            "tile_layer": {
                "default": "",
                "title": "Tile Layer",
                "type": "string",
                "description": "A named tile layer, ie OpenStreetMap."
            },
            "static": {
                "default": false,
                "title": "Static",
                "type": "boolean",
                "description": "Set to true to disable map pan/zoom."
            },
            "title": {
                "default": "",
                "title": "Title",
                "type": "string",
                "description": "The map title."
            },
            "north_arrow_props": {
                "allOf": [
                    {
                        "$ref": "#/$defs/NorthArrowProperty"
                    }
                ],
                "default": {
                    "placement": "top-left"
                },
                "title": "North Arrow Props",
                "description": "Additional arguments for configuring the North Arrow."
            }
        },
        "required": [
            "data_type"
        ],
        "type": "object"
    },
    "td_ecomap_html_url": {
        "additionalProperties": false,
        "properties": {
            "filename": {
                "anyOf": [
                    {
                        "type": "string"
                    },
                    {
                        "type": "null"
                    }
                ],
                "default": null,
                "title": "Filename",
                "description": "            Optional filename to persist text to within the `root_path`.\n            If not provided, a filename will be generated based on a hash of the text content.\n            "
            }
        },
        "required": [],
        "type": "object"
    }
}
